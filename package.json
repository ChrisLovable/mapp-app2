{
  "name": "myaiapp",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "node server.js",
    "dev:backend": "node azure-tts-proxy.js",
    "dev:replicate": "node free-image-proxy.js",
    "dev:admin": "node admin-dashboard-api.js",
    "dev:serpapi": "node serpapi-proxy.cjs",
    "dev:rapidapi": "node rapidapi-search-proxy.cjs",
    "dev:calendar-parser": "node parse-calendar-text-proxy.cjs",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:api\" \"npm run dev:backend\" \"npm run dev:replicate\" \"npm run dev:admin\" \"npm run dev:rapidapi\" \"npm run dev:calendar-parser\"",
    "build": "tsc -b && vite build",
    "build:prod": "tsc -b && vite build --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "echo \"No tests specified\" && exit 0",
    "convert-webm": "tsx scripts/convert-webm-to-mp3.ts"
  },
  "dependencies": {
    "@fullcalendar/daygrid": "^6.1.18",
    "@fullcalendar/interaction": "^6.1.18",
    "@fullcalendar/list": "^6.1.18",
    "@fullcalendar/react": "^6.1.18",
    "@fullcalendar/timegrid": "^6.1.18",
    "@supabase/supabase-js": "^2.52.0",
    "chrono-node": "^2.8.3",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "heic2any": "^0.0.4",
    "node-fetch": "^2.7.0",
    "pdfjs-dist": "^5.3.93",
    "pg": "^8.16.3",
    "react": "^19.1.0",
    "react-datepicker": "^8.4.0",
    "react-day-picker": "^9.8.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-pageflip": "^2.0.3",
    "react-router-dom": "^7.7.1",
    "react-select": "^5.10.2",
    "react-speech-recognition": "^4.0.1",
    "resend": "^4.7.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/fluent-ffmpeg": "^2.1.27",
